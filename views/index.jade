doctype html
html
  head
    block title
      title Web2Command
  body
    form(method="post", enctype="multipart/form-data", action="/upload")
      input(type="file", name="code")
      input(type="submit")
    textarea(rows="40" cols="150")= stdout
    br
    textarea(rows="10" cols="150")= stderr
    br
    textarea(rows="10" cols="150")= error
